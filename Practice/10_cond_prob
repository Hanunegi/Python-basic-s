marks1=int(input("Enter the marks in subjet 1 = "))
marks2=int(input("Enter the marks in subjet 2 = "))
marks3=int(input("Enter the marks in subjet 3 = "))

total_percentage=(100*(marks1+marks2+marks3))/300

if(total_percentage>40 and marks1>=33 and marks2>=33 and marks3>=33):
    print("hey! genius you are passed in your exams with ", total_percentage)

else:
    print("You are failed! very bad..", total_percentage)





     